# creates the VST heatmap
significantGenesHeatmap <- function(dds, label, typeOfRNA = "miRNA",
scale = FALSE, adjRow = FALSE, adjCol = FALSE)
{
print(paste("Class: ", class(dds)))
sigRes <- results(dds) %>%
data.frame %>%
filter(padj < 0.05)
dfBaseMatrix <- dds %>%
varianceStabilizingTransformation %>%
assay
if(isTRUE(scale)) {
dfBaseMatrix <- dfBaseMatrix %>% scale
}
dfBaseSig <- dfBaseMatrix %>%
data.frame %>%
rownames_to_column("gene") %>%
filter(gene %in% rownames(sigRes)) %>%
column_to_rownames("gene")
colAnnotation <- unname(sapply(colnames(dds),
function(x) { ifelse(str_detect(x, "SICK"), "red", "blue") }))
gplots::heatmap.2(
as.matrix(dfBaseSig),
trace = "none",
col = rich.colors(10),
main = paste(label, "- DESeq2 VST Counts", sep = " "),
margins = c(5, 10),
ColSideColors = colAnnotation,
key.title = "",
labRow = paste("Number of DE \n", typeOfRNA, ": ", nrow(dfBaseSig)),
labCol = "Plasma Samples",
adjRow = adjRow,
adjCol = adjCol,
srtCol = 0,
cexRow = 1.2,
cexCol = 1.2,
key.xlab = "log2 DESeq2 norm. \n read count"
)
}
sigGenesHeatmapFromDDS <- function(
dds,
contrast,
title,
margins,
scale = FALSE,
vst = FALSE,
colAnnotation = NULL
)
{
if(isTRUE(vst)) {
dfBaseMatrix <- dds %>%
varianceStabilizingTransformation %>%
assay
} else {
dfBaseMatrix <- dds %>%
assay
}
if(isTRUE(scale)) {
dfBaseMatrix <- dfBaseMatrix %>% scale
}
sigRes <- results(dds, contrast = list(contrast)) %>%
data.frame %>%
filter(padj < 0.05)
# if(is.null(colAnnotation)) {
#   colAnnotation <- rep("", ncol(sigRes))
# }
if(nrow(sigRes) == 0){
return("No significant DEG")
}
dfBaseSig <- dfBaseMatrix %>%
data.frame %>%
rownames_to_column("gene") %>%
filter(gene %in% rownames(sigRes)) %>%
column_to_rownames("gene")
if(is.null(colAnnotation)) {
colAnnotation <- rep("white",28)
}
gplots::heatmap.2(
as.matrix(dfBaseSig),
main = title,
trace = "none",
col = rich.colors(10),
ColSideColors = colAnnotation,
margins = margins,
key.title = "",
)
}
